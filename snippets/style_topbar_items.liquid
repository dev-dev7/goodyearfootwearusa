          transform:perspective(1px) translate3d(0,-30px,0) scaleY(1.1)
        }
        7%{
          -webkit-animation-timing-function:cubic-bezier(.755,.05,.855,.06);
          animation-timing-function:cubic-bezier(.755,.05,.855,.06);
          -webkit-transform:perspective(1px) translate3d(0,-15px,0) scaleY(1.05);
          transform:perspective(1px) translate3d(0,-15px,0) scaleY(1.05)
        }
        8%{
