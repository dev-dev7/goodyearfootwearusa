{%- for item in blocks -%}
  {%- case item.type -%}
    {%- when 'cart' -%}
      .vs-topbar #block-{{ item.id }} .icon-cart{
        background-color:{{ item.settings.cart_bg }};
        color:{{ item.settings.cart_color }}
      }
      @media all and (min-width:992px){
        .header #block-{{ item.id }} .icon-cart{
          background-color:{{ item.settings.cart_bg }};
          color:{{ item.settings.cart_color }}
        }
        #block-{{ item.id }} .icon-cart svg{
          fill:{{ item.settings.cart_icon }}
        }
      }
      .vs-topbar #block-{{ item.id }}.cart-wrapper:hover .icon-cart{color:#ffffff}
      .vs-topbar #block-{{ item.id }}.cart-wrapper:hover .icon-cart svg{fill:#ffffff}
      #block-{{ item.id }} .icon-cart svg{
        width:{{ item.settings.size_icon }}px;
        height:{{ item.settings.size_icon }}px
      }
      #block-{{ item.id }} .icon-cart__total{
        background-color:{{item.settings.c_amount_bg}};
        color:{{item.settings.c_amount_text}}
      }

    {%- when 'login' -%}
        #block-{{ item.id }} svg,
        #block-{{ item.id }}1 svg,
        #block-{{ item.id }}2 svg{width:{{ item.settings.size_icon }}px}

    {%- when 'language' -%}
      {%- if item.settings.lang_type == 'weglot-theme' -%}
        .weglot-container{display:none}
        .wg-default,.wg-default .country-selector{position:relative;top:auto;right:auto;bottom:auto;left:auto;}
        .wg-drop.country-selector ul{display:none}
        .wg-drop.country-selector.closed .wgcurrent::after,.wg-drop.country-selector .wgcurrent::after{content:none !important}
        .wg-flags.flag-1 a::before{-webkit-transform:scale(.5);transform:scale(.5);margin-right:0}
        .wg-drop.country-selector a{height:39px;padding:0 1.8rem 0 2.5rem !important;color:inherit;line-height:39px}
        .wg-drop.country-selector .wgcurrent{border:0}
        .wg-drop.country-selector{background-color:transparent}
        .wg-drop.country-selector:not(.closed) .wg-li.focus a, .wg-drop.country-selector:not(.closed) a:hover{color:inherit}
        .wg-drop.country-selector:not(.closed) ul{border:0;background-color:#f4f4f4}
        @media all and (max-width:992px){.wg-drop.country-selector a{height:31px;padding:0 1.2rem 0 1.2rem !important;line-height:31px}}
      {%- endif -%}

    {%- when 'search_box' -%}
      #block-{{ item.id }} .header-search .header-search__button,
      {%- if item.settings.search_btn_border != blank -%}
        #block-{{ item.id }} .header-search .header-search__input{border-width:{{ item.settings.search_btn_border }}px;}
      {%- endif -%}
      #block-{{ item.id }} .header-search__button{
        color:{{ item.settings.color_search }}
      }
      #block-{{ item.id }} .header-search__button:hover{
        color:{{ item.settings.color_search_h }}
      }
      #block-{{ item.id }} .header-search__input{
        border-color:{{ item.settings.color_search_ib }};
        color:{{ item.settings.color_search_input }}
      }
      #block-{{ item.id }} .header-search__button svg{
        width:{{ item.settings.size_icon }}px
      }
      #block-{{ item.id }} .header-search{
        background-color:{{ item.settings.color_bg }}
      }


    {%- when 'wishlist' -%}
      #block-{{ item.id }} svg{width:{{ item.settings.size_icon }}px}

    {%- when 'compare' -%}
      #block-{{ item.id }} svg{width:{{ item.settings.size_icon }}px}

    {%- when 'checkout' -%}
      #block-{{ item.id }} svg{width:{{ item.settings.size_icon }}px}


    {%- when 'dropdown' -%}
      #block-{{ item.id }} svg{width:{{ item.settings.size_icon }}px}

    {%- when 'menu_tooltip' -%}
      {%- capture tt_tf -%}
        {%- if item.settings.text_transform == 'lowercase' -%}
          text-transform:lowercase;
          line-height:1;
        {%- elsif item.settings.text_transform == 'uppercase' -%}
          top:-3px;
          padding:0 2px;
          font-size:.8rem;
          font-weight:700;
          line-height:.9rem;
          text-transform:uppercase;
        {%- endif -%}
      {%- endcapture -%}
      .menu-tt__{{ item.settings.menu_label | handleize }}::before{
        position:absolute;
        top:-7px;
        left:calc(100% + 2px);
        padding:0 1px;
        font-size:1.1rem;
        font-weight:400;
        letter-spacing:0;
        content:'{{ item.settings.tooltip_label }}';
        background-color:{{ item.settings.bg_tooltip }};
        color:{{ item.settings.c_tooltip }};
        white-space:nowrap;
        {{ tt_tf }}
      }

    {%- when 'menu_item' -%}
      #block-{{ item.id }} svg{width:{{ item.settings.size_icon }}px}

  {%- endcase -%}
{%- endfor -%}
{%- endcapture -%}
{%- assign styles = styles | strip_newlines | remove: '  ' -%}
{%- if styles != blank -%}
{{ styles }}
{%- endif -%}