  md 768px,
  lg 992px,
  xl 1200px,
  xxl 1600px,
  fs 1900px
);

$max-breakpoints: (
  xs 479px,
  sm 575px,
  smm 667px,
  md 767px,
  lg 991px,
  xl 1199px,
  xxl 1599px
);

/*============================================================*/
/*====================== *Mixins ======================*/
/*============================================================*/
@mixin mq($query, $width: 'min-width', $breakpoints: $breakpoints) {
  $breakpoint-found: false;

  @if $width == 'max' {
    $width: 'max-width';
    $breakpoints: $max-breakpoints;
  }

  @each $bp in $breakpoints {
    $name: nth($bp, 1);
    $value: nth($bp, 2);

    @if $query == $name and $value {
      $breakpoint-found: true;

      @media screen and (#{$width}: #{$value}) {
        @content;
      }
    }
  }

  @if $breakpoint-found == false {
    @warn 'Breakpoint "#{$query}" does not exist';
  }
}

@mixin x-center {
  left: 50%;
  transform: translateX(-50%);
  -webkit-transform: translateX(-50%);
}

@mixin y-center {
  top: 50%;
  transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
}

