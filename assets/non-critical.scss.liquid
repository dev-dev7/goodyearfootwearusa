// = Lookbook
.lookbook-spot {
  position: absolute;
  border-radius: 50%;
  background-color: transparent;
  z-index: 1;
  cursor: pointer;
  animation-name: border-light;
  animation-duration: 2s;
  animation-iteration-count: infinite;

  &.lookbook-spot__small {
    width: 2.9rem;
    height: 2.9rem;
  }

  &.lookbook-spot__medium {
    width: 3.8rem;
    height: 3.8rem;
  }

  &.lookbook-spot__big {
    width: 3.5rem;
    height: 3.5rem;
  }

  &.active,
  &:hover {
    .lookbook-expander,
    .lookbook-switcher {
      transition: .25s;
      background-color: $c-lookbook-hover-bg;
    }
  }

  &.active {
    z-index: 2;

    .lookbook-expander,
    .lookbook-switcher {
      -webkit-transform: rotate(45deg);
      transform: rotate(45deg);
      transition: -webkit-transform .35s ease-in;
      transition: transform .35s ease-in;
      transition: transform .35s ease-in, -webkit-transform .35s ease-in;
    }

    .lookbook-card {
      opacity: 1;
      visibility: visible;
      transition: .35s;
    }
  }

  .slideshow-lookbook-2 & { display: block; }

  @media (min-width: 568px) {
    section & { display: block; }
  }

  @media (min-width: 768px) {
    &.lookbook-spot__big {
      width: 4.9rem;
      height: 4.9rem;
    }
  }

  @media (min-width: 1200px) {
    &.lookbook-spot__big {
      width: 5.9rem;
      height: 5.9rem;
    }
  }
}

.lookbook-expander,
.lookbook-switcher {
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
  justify-content: center;
  position: absolute;
  transition: .35s ease-in;
  border-radius: 50%;
  background-color: $c-lookbook-bg;
  color: $c-lookbook-text;

  .lookbook-spot__small & {
    top: .3rem;
    left: .3rem;
    width: 2.3rem;
    height: 2.3rem;
    svg { width: 11px }
  }

  .lookbook-spot__medium & {
    top: .4rem;
    left: .4rem;
    width: 3rem;
    height: 3rem;
    svg { width: 11px }
  }

  .lookbook-spot__big & {
    top: .5rem;
    left: .5rem;
    width: 2.5rem;
    height: 2.5rem;

    svg { width: 10px }

    @media (min-width: 768px) {
      width: 3.5rem;
      height: 3.5rem;
      top: .7rem;
      left: .7rem;

      svg { width: 1.1rem }
    }

    @media (min-width: 1200px) {
      width: 4.5rem;
      height: 4.5rem;

      svg { width: 1.2rem }
    }
  }
}

