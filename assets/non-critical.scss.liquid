/*********** = Animations ***********/
@keyframes rotate {
  to { transform: rotate(360deg); -webkit-transform: rotate(360deg) }
}

// Rotate menu items
@keyframes menuGrowDown {
  0% { transform: scaleY(0); }
  80% { transform: scaleY(1.1); }
  100% { transform: scaleY(1); }
}

@keyframes menuShrinkUp {
  0% { transform: scaleY(1); opacity: 1; }
  20% { transform: scaleY(1.1); }
  100% { transform: scaleY(0); opacity: 0; }
}

@keyframes shine {
  100% { left: 125%; }
}

@-webkit-keyframes slideInBlrBttm {
  0% {
    -webkit-transform: translateY(1000px) scaleY(2.5) scaleX(0.2);
    transform: translateY(1000px) scaleY(2.5) scaleX(0.2);
    -webkit-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
    filter: blur(40px);
    opacity: 0;
  }
  100% {
    -webkit-transform: translateY(0) scaleY(1) scaleX(1);
    transform: translateY(0) scaleY(1) scaleX(1);
    -webkit-transform-origin: 50% 50%;
    transform-origin: 50% 50%;
    filter: blur(0);
    opacity: 1;
  }
}
@keyframes slideInBlrBttm {
  0% {
    -webkit-transform: translateY(1000px) scaleY(2.5) scaleX(0.2);
    transform: translateY(1000px) scaleY(2.5) scaleX(0.2);
    -webkit-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
    filter: blur(40px);
    opacity: 0;
  }
  100% {
    -webkit-transform: translateY(0) scaleY(1) scaleX(1);
    transform: translateY(0) scaleY(1) scaleX(1);
    -webkit-transform-origin: 50% 50%;
    transform-origin: 50% 50%;
    filter: blur(0);
    opacity: 1;
  }
}

@-webkit-keyframes slideOutBlrBttm {
  0% {
    -webkit-transform: translateY(0) scaleY(1) scaleX(1);
    transform: translateY(0) scaleY(1) scaleX(1);
    -webkit-transform-origin: 50% 50%;
    transform-origin: 50% 50%;
    -webkit-filter: blur(0);
    filter: blur(0);
    opacity: 1;
  }
  100% {
    -webkit-transform: translateY(1000px) scaleY(2) scaleX(0.2);
    transform: translateY(1000px) scaleY(2) scaleX(0.2);
    -webkit-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
    -webkit-filter: blur(40px);
    filter: blur(40px);
    opacity: 0;
  }
}

@keyframes slideOutBlrBttm {
  0% {
    -webkit-transform: translateY(0) scaleY(1) scaleX(1);
    transform: translateY(0) scaleY(1) scaleX(1);
    -webkit-transform-origin: 50% 50%;
    transform-origin: 50% 50%;
    -webkit-filter: blur(0);
    filter: blur(0);
    opacity: 1;
  }
  100% {
    -webkit-transform: translateY(1000px) scaleY(2) scaleX(0.2);
    transform: translateY(1000px) scaleY(2) scaleX(0.2);
    -webkit-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
    -webkit-filter: blur(40px);
    filter: blur(40px);
    opacity: 0;
  }
}

// Lookbook border flickering
@keyframes border-light {
  0% {background-color: transparent;}
  50% {background-color: rgba($color: #fff, $alpha: .36);}
  100% {background-color: transparent;}
}

// Menu animation
@keyframes slideInLeft {
  from {
    -webkit-transform: translate(-15px, 0);
    transform: translate(-15px, 0);
    opacity: 0;
  }
  to {
    -webkit-transform: translate(0, 0);
    transform: translate(0, 0);
    opacity: 1;
  }
}

// Add cart attention seekers
.product-form .btn-anm-cart {
  animation-duration: 10s;
  animation-delay: 2.7s;
  animation-iteration-count: infinite;
  transform-origin: center bottom;
}

// Loading icons
.spinner {
  position: absolute;
  margin-top: 30px;
  background-color: transparent;
  transition: none;
  z-index: 9;
  opacity: 0;
  visibility: hidden;

  &.active {
    opacity: .7;
    visibility: visible;
  }

  &::before {
    display: block;
    position: relative;
    width: 1.8rem;
    height: 1.8rem;
    margin: 0 auto;
    border-radius: 50%;
    border: 2px solid transparent;
    border-top-color: inherit;
    border-bottom-color: inherit;
    content: '';
    animation: rotate 1.25s ease infinite;
  }
}

.result-loading,
.vnd-loading-scr {
  opacity: .8;

  svg {
    display: block;
    position: relative;
    margin: 0 auto;
    z-index: 9;
  }

  &.active {
    display: block;
  }
}

.result-loading {
  top: -1.5rem;
  bottom: 0;
  left: 0;
  background-color: rgba(250,255,255,0.65);
  color: #111;

  svg {
    top: 30rem;
    width: 3.6rem;
  }
}

.vnd-loading-scr {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0,0,0,.6);
  color: #ffffff;
  z-index: 999;

  svg {
    top: 50%;
    width: 7.8rem;
    transform: translateY(-50%);
  }
}

.vnd-btn-busy {
  :not(.spinner) {
    transition: all .35s;
    opacity: 0;
    visibility: hidden;
  }

  .spinner {
    margin-top: 0;
    opacity: 1;
    visibility: visible;
    transition: all .35s;
  }
}

// Effects on Images
// Content effect image zoom in quickly
.c-e__i__zoom-in-q {
  overflow: hidden;

  .image-bg,
  .placeholder {
    transition: opacity .3s ease-in, -webkit-transform .3s ease-in, transform .3s ease-in;
    will-change: transform;
  }

  &:hover {
    .image-bg,
    .placeholder {
      -webkit-transform: scale3d(1.15, 1.15, 1);
      transform: scale3d(1.15, 1.15, 1);
    }
  }
}

// Content effect image zoom in slowly
.c-e__i__zoom-in {
  overflow: hidden;

  .image-bg,
  .placeholder {
    transition: opacity .35s linear, -webkit-transform 10.2s linear, transform 10.2s linear;
    will-change: transform;
  }

  &:hover {
    .image-bg,
    .placeholder {
      -webkit-transform: scale3d(1.15, 1.15, 1);
      transform: scale3d(1.15, 1.15, 1);
    }
  }
}

// Content effect image shine on hover
.c-e__i__shine {
  overflow: hidden;

  .image-bg::before,
  .placeholder::before {
    display: block;
    position: absolute;
    top: 0;
    left: -95%;
    width: 50%;
    height: 100%;
    -webkit-transform: skewX(-25deg);
    transform: skewX(-25deg);
    background: -webkit-linear-gradient(left, rgba(255,255,255,0) 0%, rgba(255,255,255,.3) 100%);
    background: linear-gradient(to right, rgba(255,255,255,0) 0%, rgba(255,255,255,.3) 100%);
    z-index: 2;
    content: '';
  }

  &:hover {
    .image-bg::before,
    .placeholder::before {
      -webkit-animation: shine .95s;
      animation: shine .95s;
    }
  }
}

// Content effect - image blur & brightness
.c-e__i__bb {
  .image-bg,
  .placeholder {
    transition: .3s;
  }

  &:hover {
    .image-bg,
    .placeholder {
      filter: brightness(.85) blur(2px);
    }
  }
}

// Content effect - fade/move text
.c-e__t__fm {
  .text-content {
    p,
    h3,
    h2,
    a {
      transform: translateX(-10%);
      transition: .3s;
      opacity: 0;
    }

    h2 { transition-delay: .15s; }
    p { transition-delay: .15s; }
  }

  &:hover .text-content {
    p,
    h3,
    h2,
    a {
      transform: translateX(0);
      opacity: 1;
    }
  }
}

// Content effect - text fly from left
.c-e__t__fly-left {
  .text-content {
    transform: translateX(-20%);
    transition: .3s;
    opacity: 0 !important;
  }

  &:hover .text-content {
    transform: translateX(0);
    opacity: 1 !important;
  }
}

// Content effect - fade scale appear
.c-e__fsa {
  opacity: 0;
  transform: scale(.9);
  transition: all .63s ease-in-out;

  &.c-e__fsa-a {
    opacity: 1;
    transform: scale(1);
  }
}


// Text animations
.slideshow-content-default,
.slideshow-content-adjust {
  // content effect appear from;
  &.from-top,
  &.from-right,
  &.from-bottom,
  &.from-left {
    transition: opacity .35s ease-in .75s, margin .35s ease-in .75s;

    .is-selected & {
      opacity: 1;
      margin: 0;
    }
  }

  &.from-top { margin-top: -4%; }
  &.from-right { margin-left: 6%; }
  &.from-bottom { margin-top: 4%; }
  &.from-left { margin-left: -6%; }

  &.flip-vertical {
    .is-selected & {
      opacity: 1;
    }

    .mobile-content-default,
    .mobile-content-adjust {
      .is-selected & {
        transition: .55s ease-in .75s;
        transform: rotate3d(1, 0, 0, 0);
      }
    }
  }

  &.flip-horizontal {
    .is-selected & {
      opacity: 1;
    }

    .mobile-content-default,
    .mobile-content-adjust {
      transition: .55s ease-in .75s;

      .is-selected & {
        transform: rotate3d(0, 1, 0, 0);
        opacity: 1;
      }
    }
  }
}

