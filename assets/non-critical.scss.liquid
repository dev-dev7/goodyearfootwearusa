/********** = Product Image Thumbs **********/
.thumbs-wrapper {
  .thumb-arrow__up,
  .thumb-arrow__down {
    display: block;
  }
}

.thumb-arrow__up,
.thumb-arrow__down {
  position: absolute;
  left: 50%;
  margin-left: -1.4rem;
  cursor: pointer;
  user-select: none;

  &.disabled {
    opacity: 0.7;
    cursor: initial;

    .thumbs-rect & { display: none; }
  }

  svg {
    display: block;
    width: 2.8rem;
    height: 2.8rem;
    padding: .7rem;
    border: 2px solid #555;
    fill: #555;

    .thumbs-circle & {
      border: 2px solid transparent;
      border-radius: 100%;
      fill: #777777;
      transition: .35s;
    }
  }

  &:hover svg {
    border-color: $c-theme;
    background-color: $c-theme;
    fill: #fff;

    .thumbs-circle & {
      border: 2px solid #777777;
      background-color: transparent;
      fill: #777777;
    }
  }
}

.thumb-arrow__up {
  top: .7rem;
  .thumbs-circle & { top: -1.4rem; }
}

.thumb-arrow__down {
  bottom: .7rem;
  .thumbs-circle & { bottom: -1.4rem; }
}

.product-img-area.thumbs-circle {
  align-items: center;
  -ms-flex-align: center;

  &.thumbs-left .thumbs-wrapper {
    height: 100%;
  }
}

